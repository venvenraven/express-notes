// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL") // needed for Supabase migrations
}

model Notes {
  id   Int    @id @default(autoincrement())
  name String
  body String
}
